<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirill.drawprimitives">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RectActivity"
            android:label="@string/title_activity_rect"></activity>
        <activity
            android:name=".LineActivity"
            android:label="@string/title_activity_line"></activity>
        <activity
            android:name=".CircleActivity"
            android:label="@string/title_activity_circle"></activity>
    </application>

</manifest>
